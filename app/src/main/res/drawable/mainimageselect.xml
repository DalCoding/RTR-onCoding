<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false" android:drawable="@color/fui_transparent">
        <shape>
            <corners android:radius="1dp" />
            <stroke android:width="2dp" android:color="@color/fui_transparent" />
            <size android:height="45dp" android:width="45dp" />
        </shape>
    </item>
    <!-- 클릭했을 때 -->
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <solid android:color="#F57F17" />
            <corners android:radius="1dp" />
        </shape>
    </item>
</selector>