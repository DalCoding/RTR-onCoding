<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#ACA7A3">
    <item android:id="@+id/mask2">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#4AACA7A3" />
            <solid android:color="#19EAEAEA" />
            <corners android:radius="2dp" />
        </shape>
    </item>

    <item android:id="@+id/background2">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="#00EFEBDA"
                android:startColor="#00EFEBDA"
                android:type="linear" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</ripple>